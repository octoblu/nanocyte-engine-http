{
  "name": "nanocyte-engine-http",
  "version": "3.0.4",
  "description": "HTTP server for Nanocyte that dumps messages into a queue",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "istanbul cover _mocha -- -R spec --require coffee-coverage/register-istanbul",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/nanocyte-engine-http.git"
  },
  "keywords": [
    "nanocyte",
    "meshblu"
  ],
  "author": "Octoblu, Inc <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/nanocyte-engine-http/issues"
  },
  "homepage": "https://github.com/octoblu/nanocyte-engine-http#readme",
  "devDependencies": {
    "chai": "^3.4.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "fakeredis": "^1.0.3",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-istanbul": "^0.3.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "nyc": "^8.3.0",
    "mocha-osx-reporter": "^0.1.2",
    "mocha-multi": "^0.9.1"
  },
  "dependencies": {
    "@octoblu/connect-http-signature": "^0.1.3",
    "@octoblu/redis-ns": "^1.3.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "debug": "^2.2.0",
    "errorhandler": "^1.4.2",
    "express": "^4.14.0",
    "express-meshblu-healthcheck": "^1.0.1",
    "express-package-version": "0.0.2",
    "fetch-meshblu-public-key": "^1.1.0",
    "ioredis": "^2.2.0",
    "lodash": "^4.14.0",
    "morgan": "^1.6.1",
    "node-wget": "^0.4.2",
    "octoblu-raven": "^3.2.0"
  },
  "optionalDependencies": {
    "hiredis": "^0.4.1"
  }
}
